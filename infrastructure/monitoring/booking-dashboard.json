{
  "dashboard": {
    "id": null,
    "title": "Event Booking System Dashboard",
    "tags": ["booking", "events", "nestjs"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "schemaVersion": 16,
    "version": 0,
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "timepicker": {
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "templating": {
      "list": []
    },
    "annotations": {
      "list": []
    },
    "links": [],
    "panels": [
      {
        "id": 1,
        "title": "Total Bookings",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 6,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(booking_requests_total)",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "unit": "short",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          }
        },
        "valueMappings": [],
        "mappingTypes": [
          {
            "name": "value to text",
            "value": 1
          },
          {
            "name": "range to text",
            "value": 2
          }
        ]
      },
      {
        "id": 2,
        "title": "Booking Requests",
        "type": "graph",
        "gridPos": {
          "x": 6,
          "y": 0,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "rate(booking_requests_total[5m])",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Requests/sec",
            "show": true
          }
        ]
      },
      {
        "id": 3,
        "title": "API Response Time",
        "type": "graph",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "show": true
          }
        ]
      },
      {
        "id": 4,
        "title": "Error Rate",
        "type": "graph",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status=~\"5..|4..\"}[5m])) / sum(rate(http_requests_total[5m])) * 100",
            "format": "time_series",
            "intervalFactor": 1,
            "refId": "A"
          }
        ],
        "yAxes": [
          {
            "label": "Percentage",
            "show": true
          }
        ]
      }
    ],
    "refresh": "5s"
  }
}